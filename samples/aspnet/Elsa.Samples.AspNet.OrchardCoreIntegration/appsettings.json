{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Webhooks": {
    "Sources": [
      {
        "Id": "Orchard",
        "Name": "Orchard",
        "Origin": "https://localhost:8196",
        "EventTypes": [
          {
            "EventType": "content-item.published",
            "DisplayName": "Content Item Published",
            "Description": "Received when a content item has been published.",
            "PayloadType": "Elsa.OrchardCore.WebhookPayloads.ContentItemPublished, Elsa.OrchardCore"
          }
        ]
      }
    ]
  }
}
