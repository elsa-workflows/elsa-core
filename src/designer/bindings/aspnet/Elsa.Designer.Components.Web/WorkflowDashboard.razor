<elsa-studio-root server-url="@ServerUrl" monaco-lib-path="@MonacoLibPath" use-x6-graphs="@UseX6Graphs" culture="@Culture" base-path="@BasePath">
    <elsa-studio-dashboard base-path="@BasePath"></elsa-studio-dashboard>
</elsa-studio-root>

@code{

    [Parameter]
    public string ServerUrl { get; set; } = default!;

    [Parameter]
    public string MonacoLibPath { get; set; } = "_content/Elsa.Designer.Components.Web/monaco-editor/min";
    
    [Parameter]
    public string Culture { get; set; } = default!;

    [Parameter]
    public string BasePath { get; set; } = default!;

    [Parameter]
    public bool UseX6Graphs { get; set; } = false;

}